#!/usr/bin/env ruby

require 'bundler'
Bundler.require :default, (ENV['POLYPHEMUS_ENV'] || :development).to_sym

require_relative '../lib/polyphemus'
require 'yaml'

config = YAML.load(File.read(File.expand_path("../../config.yml",__FILE__)))

class EtlToolCommand < Etna::Command
end

class EtlMagmaCommand < EtlToolCommand
  usage

  def execute(*subcommands)
  end
end

Polyphemus.instance.commands(EtlToolCommand)
Polyphemus.instance.run_command(config, Etna::Command, *ARGV)
