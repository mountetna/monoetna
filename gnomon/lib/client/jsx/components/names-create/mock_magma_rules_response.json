{
    "rules": {
        "project": "PROJECT",
        "subject": "PROJ SEP SP .n",
        "timepoint": ".subject SEP TP .n",
        "biospecimen": ".timepoint BSP .n",
        "sc_seq": ".biospecimen SEP SCA .n",
        "cytof": ".biospecimen SEP CYT .n",
        "rna_seq": ".biospecimen SEP RNA .n",
        "dna_seq": ".biospecimen SEP DNA .n"
    },
    "synonyms": [
        [
            "PROJ",
            "PROJECT"
        ]
    ],
    "tokens": {
        "SEP": {
            "name": "SEP",
            "label": "separator",
            "values": {
                "-": "# Separator"
            }
        },
        "PROJ": {
            "name": "PROJ",
            "label": "project",
            "values": {
                "EXAMPLE": "Example Project"
            }
        },
        "PROJECT": {
            "name": "PROJECT",
            "label": "project",
            "values": {
                "Example Project": "Example Project"
            }
        },
        "SP": {
            "name": "SP",
            "label": "species",
            "values": {
                "HS": "Homo sapiens",
                "MM": "Mus musculus"
            }
        },
        "TP": {
            "name": "TP",
            "label": "timepoint",
            "values": {
                "E": "Event"
            }
        },
        "BSP": {
            "name": "BSP",
            "label": "biospecimen",
            "values": {
                "PL": "Plasma",
                "SR": "Serum",
                "ETA": "Endotracheal aspirate",
                "BLD": "Whole blood",
                "NAS": "Nasal swab",
                "PBMC": "PBMCs"
            }
        },
        "SCA": {
            "name": "SCA",
            "label": "single-cell assay",
            "values": {
                "SCG": "Single-cell Gene Expression",
                "SCB": "Single-cell BCR Seq ",
                "SCT": "Single-cell TCR Seq ",
                "SCC": "Single-cell CITE-Seq",
                "SNA": "Single Nuclear ATAC-Seq (Standalone)",
                "SCM": "Single Cell Multiplexing output (HTO, MS)",
                "SCA": "Single-cell ATAC-Seq (Multiome)"
            }
        },
        "CYT": {
            "name": "CYT",
            "label": "cytof assay",
            "values": {
                "CYM": "Cytof",
                "CYF": "Flow Cytometry",
                "CYS": "Spectral Flow Cytometry"
            }
        },
        "RNA": {
            "name": "RNA",
            "label": "rna-seq assay",
            "values": {
                "RSQ": "Bulk RNA Seq"
            }
        },
        "DNA": {
            "name": "DNA",
            "label": "dna-seq assay",
            "values": {
                "WXS": "Whole Exome Seq",
                "WGS": "Whole Genome Seq",
                "WGM": "Whole Genome Methylation",
                "CHP": "ChIP Seq"
            }
        }
    }
}